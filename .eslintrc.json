{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "rules": {
      "no-underscore-dangle": [
        "error",
        { "allow": ["_id"] }
      ]
    }
}
